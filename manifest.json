{
  "manifest_version": 2,

  "name": "My awesome weather extension",
  "description": "Shows the local weather data when a new tab is opened.",
  "version": "0.1.0",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },

  "permissions": [
    "geolocation"
  ],
  "content_security_policy": "script-src 'self' http://maps.googleapis.com/maps/api/; object-src 'self'",

  "web_accessible_resources": ["newTab.html"]





}