{
  "manifest_version": 2,

  "name": "My awesome weather extension",
  "description": "Shows the local weather data when a new tab is opened.",

  "permissions": [
    "sessions"
  ],
  
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },

  "background":{
    "scripts": "background.js"
  },

  "web_accessible_resources": ["newTab.html"]





}